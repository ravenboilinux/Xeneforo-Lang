msgstr "test"

#: [XenForo][option_defaultEmailAddress][null][0][]
msgid "Default Email Address [option_defaultEmailAddress][0]"
msgstr "Default Email Address"

#: [XenForo][option_defaultEmailAddress_explain][null][0][]
msgid "This is the default email address that emails will be sent from. [option_defaultEmailAddress_explain][0]"
msgstr "This is the default email address that emails will be sent from."

#: [XenForo][option_defaultLanguageId][null][0][]
msgid "Default Language [option_defaultLanguageId][0]"
msgstr "Default Language"

#: [XenForo][option_defaultLanguageId_explain][null][0][]
